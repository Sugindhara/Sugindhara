#include <stdio.h>

int main() 
{
    int n, t1=0,t2=1,next_term;
    printf("Enter the number of terms  ");
    scanf("%d",&n);
    printf("fibonacci series:\n");
    for(int i=1;i<=n;i++)
    {
        printf("%d \n",t1);
        next_term=t1+t2;
        t1=t2;
        t2=next_term;
    } 
  return 0;
  
}
